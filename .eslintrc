{
    "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
    "plugins": ["prettier", "jest"],
    "rules": {
      "prettier/prettier": ["warn"],
      "import/no-unresolved": ["off"],
      "import/no-extraneous-dependencies": ["off"],
      "no-console": ["off"],
      "react/jsx-filename-extension": ["off"]
    },
    "env" : {
        "browser": true
    }
  }