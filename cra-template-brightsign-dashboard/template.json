{
    "package": {
        "scripts": {
            "build": "webpack --mode=development",
            "build:prod": "webpack --mode=production --node-env=production",
            "clean": "rm -rf dist",
            "reinstall": "npm run clean && rm -rf node_modules && npm install",
            "watch": "webpack --watch",
            "put": "npm run build && ./scripts/put",
            "put:prod": "npm run build:prod && ./scripts/put",
            "put:watch": "nodemon -e js,brs,css --watch src --watch package.json --exec 'npm put:prod'"
          },
          "devDependencies": {
            "@babel/core": "^7.23.0",
            "@babel/preset-env": "^7.22.20",
            "@babel/preset-react": "^7.23.3",
            "@webpack-cli/generators": "^3.0.2",
            "babel-loader": "^9.1.3",
            "html-webpack-plugin": "^5.5.3",
            "webpack": "^5.82.0",
            "webpack-cli": "^5.1.0",
            "nodemon": "^3.0.3",
            "css-loader": "^6.10.0",
            "style-loader": "^3.3.4"
          },
          "engines": {
            "node": "<=14.17.6"
          },
          "dependencies": {
            "node": "^14.17.6",
            "nvm": "^0.0.4"
          }
    }
  }